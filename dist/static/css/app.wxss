page {
  height: 100%;
  line-height: 1;
  font-family: 'PingFang SC', 'STHeitiSC-Light', 'Helvetica-Light', 'arial', 'sans-serif', 'Droid Sans Fallback', 'Helvetica neue', 'Microsoft Yahei', 'Helvetica', 'Tahoma', 'lantinghei sc';
  -webkit-tap-highlight-color: transparent;
  background: #fff;
}
navigator {
  color: #7e8c8d;
}
.navigator-hover {
  background-color: #fff;
  opacity: 1;
}
.zan-form {
  background-color: #fff;
  border-top: 1rpx solid #e5e5e5;
  border-bottom: 1rpx solid #e5e5e5;
}
.zan-form__item {
  line-height: 52rpx;
}
.zan-form__title {
  float: left;
  width: 160rpx;
  font-size: 28rpx;
}
.zan-form__title--top {
  align-self: flex-start;
}
.zan-form__input,
.zan-form__textarea {
  min-height: 52rpx;
}
.zan-form__input ._input {
  min-height: 52rpx;
}
.zan-form__textarea {
  display: -webkit-flex;
  display: flex;
  align-items: center;
}
.zan-form__textarea ._textarea {
  width: auto;
  flex: 1;
}